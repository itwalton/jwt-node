{
  "name": "@itwalton/jwt-node",
  "version": "1.0.5",
  "description": "Issue and verify access tokens in Node",
  "main": "dist",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc",
    "start:dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:md": "markdownlint '**/*.md'",
    "lint:all": "concurrently \"npm run lint\" \"npm run lint:md\"",
    "prepare": "husky install"
  },
  "publishConfig": {
    "@itwalton:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/itwalton/jwt-node.git"
  },
  "author": "Ian Walton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itwalton/jwt-node/issues"
  },
  "homepage": "https://github.com/itwalton/jwt-node#readme",
  "dependencies": {
    "base64url": "^3.0.1",
    "date-fns": "^2.28.0"
  },
  "devDependencies": {
    "@types/base64url": "^2.0.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^6.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "markdownlint-cli": "^0.28.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.7.2"
  }
}
